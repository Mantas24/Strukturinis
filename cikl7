#include <iostream>
#include <cmath>  // Naudojama apvalinimui
using namespace std;
int main()
{
   //ivedu kintamajį
   double sum = 1000;
   double sumDalis = 0.05;
   int metai = 10;
   
    
   //kuriame cikla
   for (int i = 1; i <= metai; i++) {
    sum += sum * sumDalis;
   }
  
   cout << "Pinigų suma po " << metai << " metų bus " << sum << endl;

    return 0;
}