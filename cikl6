#include <iostream>
#include <cmath>  // Naudojama apvalinimui
using namespace std;
int main()
{
    int sum;
    for (int i = 1; i <= 20; i++) {
        if (i % 2){
            sum += i; 
        }
    }


    cout << "Vis킬 nelygini킬 intervalo [1; 20] skai훾i킬 suma yra " << sum << endl; 

    return 0;

}