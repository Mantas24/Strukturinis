#include <iostream>
using namespace std;
int main ()
{
    // ivedu kintamaji
    double paz; 
     
    cout << "Įveskite pažimį: ";
    cin >> paz;

    // patikrinu reiksmes

    if (paz == 10){
        cout << "Puiku" << endl;
    }else if (paz >= 9){
        cout << "Labai gerai" << endl;
    }else if (paz >= 7){
        cout << "Gerai" << endl;
    }else if (paz >= 5){
        cout << "Patenkinamai" << endl;
    }else if (paz < 5){
        cout << "Egzaminas neišlaikytas" << endl;
    }     
  
  return 0;
}